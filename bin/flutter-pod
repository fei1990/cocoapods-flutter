if $PROGRAME_NAME == __FILE__ENV["BUNDLE_PATH"] ||= "./.vendor/bundle"
  ENV["BUNDLE_PATH"] ||= "./vendor/bundle"
  ENV["BUNDLE_GEMFILE"] = File.expand_path("../Gemfile", __dir__)
  require "bundler/setup"
end

